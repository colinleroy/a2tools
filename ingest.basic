10 INPUT "Data size: ";DSIZE
20 DIM D$(DSIZE)
30 PRINT "Ready to read"
40 FOR I = 1 TO DSIZE
50 GET D$(I)
60 NEXT I
70 PRINT "Read all"

100 PRINT "Writing file"
110 PRINT CHR$(4);"OPEN INFILE"
120 PRINT CHR$(4);"WRITE INFILE"

200 FOR I = 1 TO DSIZE
210 PRINT D$(I);
220 NEXT I

300 PRINT CHR$(4);"CLOSE INFILE"

400 PRINT "Done"
