#!/bin/bash

BRANCH=$(git rev-parse --abbrev-ref HEAD)
if [ "$BRANCH" != "master" ]; then
	echo Not on release branch
	exit 1
fi
rm *.dsk
make clean all dist upload && make clean all dist upload IIGS=1 || exit

#Only mastodon is ported to old II yet
cd src/mastodon
make clean all OLDII=1
cd ../../
make mastodon-oldii.dsk OLDII=1
